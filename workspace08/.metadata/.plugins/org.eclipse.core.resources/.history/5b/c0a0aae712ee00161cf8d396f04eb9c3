package com.fannie.io;

import java.io.File;
import java.io.IOException;

public class FileExample {
	public static void main(String[] args) {
		File f = new File("sample2.txt");
		
		try {
			System.out.println(f.createNewFile());
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		f.delete();
	}
}
