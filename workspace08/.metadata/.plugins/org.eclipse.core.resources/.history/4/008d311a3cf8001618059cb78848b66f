package com.fannie.bean;


public class Account {
	private int accID;
	private double actBal;
	private double avgActBal;
	public Account(int accID, double actBal, double avgActBal) {
		super();
		this.accID = accID;
		this.actBal = actBal;
		this.avgActBal = avgActBal;
	}
	@Override
	public String toString() {
		return "Account [accID=" + accID + ", actBal=" + actBal + ", avgActBal=" + avgActBal + "]";
	}
	public int getAccID() {
		return accID;
	}
	public double getActBal() {
		return actBal;
	}
	public double getAvgActBal() {
		return avgActBal;
	}
	
	
	
}
