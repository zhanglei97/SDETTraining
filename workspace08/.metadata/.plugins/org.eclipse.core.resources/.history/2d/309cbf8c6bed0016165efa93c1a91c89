package com.fannie.excep;

public class exp1 {
	public static void main(String[] args) {
		int num1=20;
		int num2 = 0;
		
		try{
			int res=num1/num2;
			
			System.out.println("result is "+res);	
		}catch(Exception e){
			System.out.println("error " + res+" "+e);
		}
		finally{
			System.out.println("someother business logic should be here too: ");
		}
		
		
		
	}

}
