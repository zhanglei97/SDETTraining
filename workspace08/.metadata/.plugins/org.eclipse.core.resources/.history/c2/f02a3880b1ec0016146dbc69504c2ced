package com.fannie.inhe;

public class CarClient {
	public static void main(String[] args){
		Car bmw = new Car("BMW");
		bmw.sterling();
		bmw.move();
		System.out.println("my car is "+bmw);
		
		Jeep chenoky = new Jeep("Blue", "Jeep", 4, 4);
		chenoky.move();
		System.out.println(chenoky);
		System.out.println("jeep doors: "+chenoky.getDoors());
	}
}
