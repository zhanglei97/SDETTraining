			System.out.println("-----------------------------");
			temp.submitLoan();
			temp.loanAmount();
		}
		
		
		System.out.println("\n!!!!!!!!!!!!!!!!!!!!!!!!!!!\n");
		loans[1].foreClosure();
		loans[2].foreClosure(1333);
		
